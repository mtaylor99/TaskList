@model AspNetGroupsViewModel

@{
    Layout = "_Layout";
}

@{
    ViewData["Title"] = "Edit Asp Net Group";
}

<form asp-action="Edit" method="post">
    <div class="TL-Main">
        <div class="TL-Header">
            <h2>Edit Asp Net Group</h2>
        </div>
        <div class="TL-Content">
            <input type="hidden" asp-for="AspNetGroup.GroupId" />
            @Html.Partial("_AspNetGroupCreateEditPartial", Model)
        </div>
        <div class="TL-Footer">
            <button type="submit" class="btn btn-primary">Save</button>
            <a asp-action="List" class="btn btn-secondary">Cancel</a>
        </div>
    </div>
</form>

