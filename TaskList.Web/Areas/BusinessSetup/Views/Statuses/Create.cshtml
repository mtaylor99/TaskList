@model TaskList.DAL.Models.Status

@{
    Layout = "_Layout";
}

@{
    ViewData["Title"] = "Create Status";
}

<div asp-validation-summary="All" class="text-danger"></div>

<form asp-action="Create" method="post">
    <div class="TL-Main">
        <div class="TL-Header">
            <h2>Create Status</h2>
        </div>
        <div>
            <div asp-validation-summary=" All" class="text-danger"></div>
        </div>
        <div class="TL-Content">
            @Html.Partial("_StatusCreateEditPartial", Model)
        </div>
        <div class="TL-Footer">
            <button type="submit" class="btn btn-primary">Save</button>
            <a asp-action="List" class="btn btn-secondary">Cancel</a>
        </div>
    </div>
</form>